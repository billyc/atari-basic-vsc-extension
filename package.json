{
	"name": "atari-fastbasic",
	"displayName": "Atari FastBasic",
	"description": "Language support for Atari FastBasic",
	"galleryBanner": {
		"color": "#04F9AE"
	},
	"icon": "icon.png",
	"publisher": "BillyCharlton",
	"repository": {
		"url": "https://github.com/billyc/atari-basic-vsc-extension.git",
		"type": "git"
	},
	"version": "1.1.2",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "basic",
				"aliases": [
					"Atari FastBasic",
					"FastBasic",
					"basic"
				],
				"extensions": [
					".bas",
					".fb"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "basic",
				"scopeName": "source.basic",
				"path": "./syntaxes/basic.tmLanguage.json"
			}
		]
	}
}
